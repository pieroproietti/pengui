#!/bin/bash 
source bin/program
cd ~/${PROGRAM}

# Create virtual environment
python3 -m venv venv_${PROGRAM}

# Install dependencies
source venv_${PROGRAM}/bin/activate

# install packages
pip install psutil
pip install pyinstaller
pip install pyside6
pip install pyyaml
#pip install pyudev
pip install requests
pip install stickytape
pip install wheel
deactivate
